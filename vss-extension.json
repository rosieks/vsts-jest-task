{
  "manifestVersion": 1,
  "id": "vsts-jest-task",
  "name": "Jest Test",
  "version": "0.2.10",
  "publisher": "Slawomirrosiek",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Delightful JavaScript Testing",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "images/jest-outline.png"
  },
  "branding": {
    "color": "#99424f",
    "theme": "dark"
  },
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "tags": [
    "jest",
    "test",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/rosieks/vsts-jest-task"
  },
  "files": [
    {
      "path": "jest/index.js"
    },
    {
      "path": "jest/task.json"
    },
    {
      "path": "jest/icon.png"
    }
  ],
  "contributions": [
    {
      "id": "jest-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "jest"
      }
    }
  ]
}